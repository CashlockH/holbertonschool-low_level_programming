#include <stdlib.h>
#include <time.h>
/**
 * main - describes the last digit of n
 * Return: 0
 */
int main(void)
{
int n;
srand(time(0));
n = rand() - RAND_MAX / 2;
if ((n % 10) > 5)
printf("Last digit of n is greater than 5")
else if ((n % 10) == 0)
printf("Last digit of n is 0)
else
printf("Last digit of n is less than 6 and not 0")
return (0);
}
